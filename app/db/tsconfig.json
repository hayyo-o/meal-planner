{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node"],
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": ["prisma/**/*.ts"]
}


